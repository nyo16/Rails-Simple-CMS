<% content_for :custom_styling do %>
  <%= javascript_include_tag "jquery.fancybox", "jquery.mousewheel-3.0.6.pack"%>
  <script type="text/javascript">
		$(document).ready(function() {
			$('.fancybox').fancybox();
		});
	</script>
<% end %>
<% content_for :header do %>
  <%= stylesheet_link_tag "jquery.fancybox", "custom_gallery"%>
<% end %>
<div class="cms_gallery">
    <% gallery.gallery_images.each do |gal| %>
      <%= link_to (image_tag gal.image.url(:thumb)), gal.image.url, :class => "fancybox", "data-fancybox-group" => "gallery" %>
    <% end %>
</div>
<div style="clear: both"></div>

